@model IEnumerable<BikeShop.Models.ViewModels.TempDateMadeViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DATEVALUE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MADECOUNT)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DATEVALUE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MADECOUNT)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.DATEVALUE }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.DATEVALUE })
        </td>
    </tr>
}

</table>
