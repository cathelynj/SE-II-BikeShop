@model IEnumerable<BikeShop.Models.ViewModels.TubeMaterialViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TUBEID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MATERIAL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESCRIPTION)
